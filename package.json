{
  "name": "flutter-assets-gen",
  "displayName": "flutter-assets-gen",
  "description": "A plugin to generate flutter assets class file by pubspec.yaml",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-assets-gen.watch",
        "title": "Watch",
        "category": "Flutter Assets"
      },
      {
        "command": "flutter-assets-gen.generate",
        "title": "Generate",
        "category": "Flutter Assets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "precompile": "rm -rf out && node ./dir.js && cp -r templates/ out/templates/",
    "compile": "tsc --build -p ./",
    "prewatch": "rm -rf out && node ./dir.js && cp -r templates/ out/templates/",
    "watch": "tsc -w -p ./",
    "lint": "eslint src --ext ts",
    "start": "node ./out/extension.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.2",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "chokidar": "^3.5.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "readline": "^1.3.0"
  }
}
